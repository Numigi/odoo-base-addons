<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <function model="ir.config_parameter"
              name="set_param"
              eval="('ir_attachment.location', 's3')" />

    <function model="ir.config_parameter"
              name="set_param"
              eval="('ir_attachment.location.host', 'minio:9000')" />

    <function model="ir.config_parameter"
              name="set_param"
              eval="('ir_attachment.location.region', 'us-east-1')" />

    <function model="ir.config_parameter"
              name="set_param"
              eval="('ir_attachment.location.access_key', 'minio')" />

    <function model="ir.config_parameter"
              name="set_param"
              eval="('ir_attachment.location.secret_key', 'miniosecret')" />

    <function model="ir.config_parameter"
              name="set_param"
              eval="('ir_attachment.location.secure', '')" />

</odoo>
